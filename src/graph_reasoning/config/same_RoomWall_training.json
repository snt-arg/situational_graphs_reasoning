{
    "report":{
        "save": true,
        "name": "RoomWall",
        "target_concept": "RoomWall"
    },
    "hdata": {
        "nodes": [
            "ws"
        ],
        "edges": [
            [
                "ws",
                "ws_same_room",
                "ws"
            ],[
                "ws",
                "ws_same_wall",
                "ws"
            ]
        ]
    },
    "random_link_split": {
        "num_val": 0.1,
        "num_test": 0.1,
        "disjoint_train_ratio": 0.0,
        "neg_sampling_ratio": 0.0,
        "add_negative_train_samples" : true,
        "is_undirected": true
    },
    "link_neighbor_loader": {
        "num_neighbors": [15,15],
        "neg_sampling_ratio": 0.0,
        "batch_size": 128,
        "shuffle": true
    },
    "gnn": {
        "encoder": {
            "nodes": {
                "hidden_channels": 16,
                "heads": [8,1],
                "dropout": 0.0,
                "input_channels": 1
            },
            "edges": {
                "hidden_channels": 16,
                "input_channels": 4
            },
            "aggr" : "sum"
        },
        "decoder": {
            "hidden_channels": [128,64],
            "output_channels" : 3
        },
        "use_cuda" : true,
        "lr" : 0.005
    },
    "training": {
        "epochs" : 300,
        "patience": 30
    },
    "hyperp_bay_optim": {
        "mapping": {"lr" : ["gnn", "lr"],
            "enc_nod_hc" : ["gnn", "encoder", "nodes", "hidden_channels"],
            "enc_edg_hc" : ["gnn", "encoder", "edges", "hidden_channels"],
            "dec_hc_0" : ["gnn", "decoder", "hidden_channels", 0],
            "dec_hc_1" : ["gnn", "decoder", "hidden_channels", 1]
        },
        "range": {"lr": {"limits": [ 0.00001, 0.0001], "num_type": "float", "log": true},
            "enc_nod_hc": {"limits": [ 8, 32], "num_type": "int"},
            "enc_edg_hc": {"limits": [ 8, 32], "num_type": "int"},
            "dec_hc_0": {"limits": [ 64, 256], "num_type": "int"},
            "dec_hc_1": {"limits": [ 32, 128], "num_type": "int"}
        }
    }
}

